            "body": {
                "type": "text",
                "raw": "-- ------------------------------\n-- OPTION\n-- ------------------------------\n\nOPTION IMPORT;\n\n-- ------------------------------\n-- LOGINS\n-- ------------------------------\n\nDEFINE LOGIN user1 ON DATABASE PASSHASH '$argon2id$v=19$m=19456,t=2,p=1$GPTt6sojtjquQpAC3FH+pA$v0DUPfh8sOxF4lVH4eWcRKSQjQcnTKtEWSwkaCsLup0';\n\n-- ------------------------------\n-- SCOPES\n-- ------------------------------\n\nDEFINE SCOPE scope1 SESSION 0ns SIGNIN (SELECT * FROM user WHERE email = $email AND crypto::argon2::compare(pass, $pass));\nDEFINE SCOPE scope2 SESSION 12h SIGNUP (CREATE user SET email = $email, pass = crypto::argon2::generate($pass)) SIGNIN (SELECT * FROM user WHERE email = $email AND crypto::argon2::compare(pass, $pass));\n\n-- ------------------------------\n-- TABLE: complaints\n-- ------------------------------\n\nDEFINE TABLE complaints SCHEMALESS PERMISSIONS NONE;\n\n-- ------------------------------\n-- TABLE: tags\n-- ------------------------------\n\nDEFINE TABLE tags SCHEMALESS PERMISSIONS NONE;\n\n-- ------------------------------\n-- TABLE: user\n-- ------------------------------\n\nDEFINE TABLE user SCHEMAFULL PERMISSIONS FOR select, update WHERE id = $auth.id, FOR create, delete NONE;\n\nDEFINE FIELD email ON user TYPE string;\nDEFINE FIELD pass ON user TYPE string;\nDEFINE FIELD user ON user TYPE string;\n\nDEFINE INDEX idx_user ON user FIELDS email UNIQUE;\n\n-- ------------------------------\n-- TRANSACTION\n-- ------------------------------\n\nBEGIN TRANSACTION;\n\n-- ------------------------------\n-- TABLE DATA: complaints\n-- ------------------------------\n\nUPDATE complaints:0o14dl9jaj26qu95p4t9 CONTENT { complaint: 'Everything to some extent.', id: complaints:0o14dl9jaj26qu95p4t9, submissionTime: '2023-06-13T01:20:46.267Z', tags: [tags:nqjuqymico3236bwweb1, tags:9hnf78w5apnmkyfen4sc] };\nUPDATE complaints:14msshpm6zci5v5evyfm CONTENT { complaint: 'Cloud Ops', id: complaints:14msshpm6zci5v5evyfm, submissionTime: '2023-06-09T22:00:19.170Z', tags: [tags:hqbolbfi47x3fxzi28cj, tags:qy716ex9gn6ui7jz90l4] };\nUPDATE complaints:6qeyyofkk1rcsme9701a CONTENT { complaint: 'Grr Anger\nwith a new line', id: complaints:6qeyyofkk1rcsme9701a, submissionTime: '2023-06-17T04:36:04.178Z', tags: [tags:g1a3n906xolw5y10bo7c, tags:qy716ex9gn6ui7jz90l4, tags:nqjuqymico3236bwweb1, tags:qn1dfvgwd8lcs679m0ai] };\nUPDATE complaints:8blw5bkj7s5kzpnv8zuq CONTENT { complaint: 'Not enough time!', id: complaints:8blw5bkj7s5kzpnv8zuq, submissionTime: '2023-06-17T02:01:18.552Z', tags: [tags:79p9vd3to4ts2gzdeule, tags:hqbolbfi47x3fxzi28cj, tags:g1a3n906xolw5y10bo7c] };\nUPDATE complaints:bqyqq4fsgmndrtm6i6ye CONTENT { complaint: 'This is a really long rant because I want to see how the word wrapping works when something this size is put into the table.', id: complaints:bqyqq4fsgmndrtm6i6ye, submissionTime: '2023-06-09T23:28:29.782Z', tags: [tags:s9fprrl11rghtc0e07jy] };\nUPDATE complaints:fcbxmyhc18zbrotuzrm8 CONTENT { complaint: 'asdf', id: complaints:fcbxmyhc18zbrotuzrm8, submissionTime: '2023-06-09T22:10:22.672Z', tags: [tags:s9fprrl11rghtc0e07jy, tags:qy716ex9gn6ui7jz90l4] };\nUPDATE complaints:ihqgl947eq3pgoeqdn1i CONTENT { complaint: 'Hopefully this works', id: complaints:ihqgl947eq3pgoeqdn1i, submissionTime: '2023-06-09T22:10:32.588Z', tags: [tags:qn1dfvgwd8lcs679m0ai, tags:9hnf78w5apnmkyfen4sc] };\nUPDATE complaints:lghirn8rcp69uorp63jz CONTENT { complaint: 'New thing is annoying', id: complaints:lghirn8rcp69uorp63jz, submissionTime: '2023-06-13T10:01:46.032Z', tags: [tags:79p9vd3to4ts2gzdeule, tags:hqbolbfi47x3fxzi28cj, tags:qy716ex9gn6ui7jz90l4, tags:e9gqtrquv7o182wflzvp] };\nUPDATE complaints:n03ldj4p524ud8xfhek8 CONTENT { complaint: 'Something bad again', id: complaints:n03ldj4p524ud8xfhek8, submissionTime: '2023-06-09T12:00:00Z', tag: [tags:nqjuqymico3236bwweb1], tags: [tags:e9gqtrquv7o182wflzvp, tags:qn1dfvgwd8lcs679m0ai] };\nUPDATE complaints:v7sl5x9stsxu46z9bn29 CONTENT { complaint: 'Performanc