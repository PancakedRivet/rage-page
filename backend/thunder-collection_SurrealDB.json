        {
            "_id": "c03f7ac3-d0da-4e04-ad8e-32da633e2fc0",
            "colId": "b8b1a967-a6d8-4cc1-88df-f2eaee824328",
            "containerId": "ff4205c1-c09d-4a90-862c-bb1cd0c7d4e7",
            "name": "Group Complaints into Time Bucket",
            "url": "http://127.0.0.1:9000/sql",
            "method": "POST",
            "sortNum": 30000,
            "created": "2023-06-14T00:57:39.831Z",
            "modified": "2023-06-16T05:46:37.784Z",
            "headers": [
                {
                    "name": "Accept",
                    "value": "application/json"
                },
                {
                    "name": "NS",
                    "value": "test"
                },
                {
                    "name": "DB",
                    "value": "test"
                }
            ],
            "params": [],
            "body": {
                "type": "text",
                "raw": "// Setting the bucket (e.g. \"day\", \"month\")\nLET $bucket = \"day\";\n\nLET $endDateTime = time::group(time::now(), $bucket);\nLET $startDateTime = $endDateTime - 1w;\n\n// Select the complaints from between the specified dates\nLET $complaintDateRange = SELECT * FROM complaints WHERE submissionTime > $startDateTime AND submissionTime < $endDateTime;\n\n// Perform groupings required to make the final result return\nLET $complaintBucket = SELECT *, time::group(submissionTime, $bucket) AS timeBucket, tags.name as tags FROM $complaintDateRange SPLIT tags;\nLET $complaintTagList = SELECT count() as total, timeBucket, tags AS tag FROM $complaintBucket GROUP BY timeBucket, tag;\nLET $tagList = SELECT tag FROM $complaintTagList;\n\n// Return the list of filterred tags\nLET $result = SELECT * FROM $complaintTagList;\n// Return a list of unique tags from the group\nLET $metaTagList = SELECT tag FROM array::distinct($tagList);\n// Return the max and minimum date bucket from the list with the timeperiod\nLET $metaTime = SELECT * FROM { timePeriod: $bucket, minDateTime: $startDateTime, maxDateTime: $endDateTime };\n\n// Return the combined result object\nSELECT * FROM { result: $result, metadata: { time: $metaTime, tagList: $metaTagList } };\n",
                "form": []
            },
            "auth": {
                "type": "basic",
                "basic": {
                    "username": "root",
                    "password": "root"
                }
            },
            "tests": []
