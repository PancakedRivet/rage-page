        {
            "_id": "d952d9f5-0be3-4f6a-9066-6192fb44e3dd",
            "colId": "7b551422-d43e-4d42-869a-cb91cb4be9f5",
            "containerId": "c61937f7-e476-4978-b690-34dd0337df84",
            "name": "Group Complaints into Time Bucket",
            "url": "http://127.0.0.1:9000/sql",
            "method": "POST",
            "sortNum": 30000,
            "created": "2023-06-17T21:22:22.577Z",
            "modified": "2023-06-17T21:31:56.357Z",
            "headers": [
                {
                    "name": "Accept",
                    "value": "application/json"
                },
                {
                    "name": "NS",
                    "value": "test"
                },
                {
                    "name": "DB",
                    "value": "test"
                }
            ],
            "params": [],
            "body": {
                "type": "text",
                "raw": "// Setting the bucket (e.g. \"day\", \"month\")\nLET $bucket = \"day\";\n\nLET $endDateTime = time::group(time::now(), $bucket) + 1d;\nLET $startDateTime = $endDateTime - 1w;\n\n// Select the complaints from between the specified dates\nLET $complaintDateRange = SELECT * FROM complaints WHERE submissionTime > $startDateTime AND submissionTime < $endDateTime;\n\n// Perform groupings required to make the final result return\nLET $complaintBucket = SELECT *, time::group(submissionTime, $bucket) AS timeBucket, tags.name as tags FROM $complaintDateRange SPLIT tags;\nLET $complaintTagList = SELECT count() as total, timeBucket, tags AS tag FROM $complaintBucket GROUP BY timeBucket, tag;\nLET $tagList = SELECT tag FROM $complaintTagList;\n\n// Return the list of filterred tags\nLET $lineGraphData= SELECT * FROM $complaintTagList;\n// Return a list of unique tags from the group\nLET $metaTagList = SELECT tag FROM array::distinct($tagList) ORDER BY tag DESC;\n// Return the max and minimum date bucket from the list with the timeperiod\nLET $metaTime = SELECT * FROM { timePeriod: $bucket, minDateTime: $startDateTime, maxDateTime: $endDateTime };\n\n// Return the combined result object\nSELECT * FROM { graphData: { line: $lineGraphData, pie: \"someData\" }, metadata: { time: $metaTime, tagList: $metaTagList } };\n",
                "form": []
            },
            "auth": {
                "type": "basic",
                "basic": {
                    "username": "root",
                    "password": "root"
                }
