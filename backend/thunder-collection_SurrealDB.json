            "params": [],
            "body": {
                "type": "text",
                "raw": "// Create a new complaint to use an existing tag\n#CREATE complaints:testcomplaint SET tags = [tags:pljp4w8hqifnr6v9ifpp];\n\n// Setting an existing complaint to use an existing tag\n#UPDATE complaints:7hqa164i6vh1fq1crguk SET tags = [tags:nqjuqymico3236bwweb1];\n\n// Query for the tags used by each complaint\n#SELECT tags FROM complaints;\n\n// Query for the tag names used by each complaint\n#SELECT tags.tag FROM complaints;\n\n// Query for complaints with a specific tag (returns tag IDs)\n#SELECT * FROM complaints WHERE tags.name CONTAINS \"Test Tag\";\n// Query for complaints with a specific tag (returns tag names)\n#SELECT *, tags.name AS tags FROM complaints WHERE tags.name CONTAINS \"Test Tag\";\n\nSELECT * FROM tags;\nSELECT id, name FROM tags WHERE name INSIDE \"Test\";\nSELECT id, name FROM tags WHERE tags CONTAINSANY \"Test\";\n\n// Setting the bucket (e.g. \"day\", \"month\")\nLET $bucket = \"day\";\n\nLET $endDateTime = time::group(time::now(), $bucket);\nLET $startDateTime = $endDateTime - 1w;\n#LET $complaintsInDateRange = SELECT *, tags.name as tags, time::group(submissionTime, $bucket) AS timeBucket FROM $complaintDateRange, tags WHERE submissionTime > $startDateTime AND submissionTime < $endDateTime;",
                "form": []
